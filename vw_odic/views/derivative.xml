<?xml version="1.0"?>
<odoo>
	<record id="vw_odic_derivative_view_tree" model="ir.ui.view">
		<field name="name">vw_odic.derivative.tree</field>
		<field name="model">vw_odic.derivative</field>
		<field name="arch" type="xml">
			<tree string="Derivative">
				<field name="text" />
				<field name="language" />
				<field name="regions_ids" widget="many2many_tags" />
				<field name="domains_ids" widget="many2many_tags" />
			</tree>
		</field>
	</record>
